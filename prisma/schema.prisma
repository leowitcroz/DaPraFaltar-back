generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model materias {
  id     Int    @id @default(autoincrement())
  name   String @db.VarChar(45)
  horas  String @db.VarChar(45)
  faltas String @db.VarChar(45)

  @@map("materias")
}
